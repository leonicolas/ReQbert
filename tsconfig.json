{
  "compileOnSave": false,
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": [
          "DOM",
          "ESNext"
      ],
      "target": "ESNext",
      "module": "NodeNext",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitThis": false,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "strict": true,
  }
}

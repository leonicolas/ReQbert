{
  "module": "commonjs",
  "compileOnSave": false,
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": [
          "dom",
          "es2015",
          "es2016"
      ],
      "target": "es2016",
      "module": "es2015",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noImplicitThis": false,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "strict": false,
  }
}
